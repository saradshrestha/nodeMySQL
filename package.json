{
  "scripts": {
    "start": "nodemon app.js" ,
    "generateKey":"node config\\secretKeyGenerator.js",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:fresh" : "npx sequelize-cli db:migrate:undo npx sequelize-cli db:migrate",

    "seed:create" : "npx sequelize-cli seed:generate --name",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.7.1",
    "nodemailer": "^6.9.13",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.2"
  }
}
